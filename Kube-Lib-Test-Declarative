import com.esg.utils.PodTemplates

slaveTemplates = new PodTemplates()

pipeline {
  agent {
    kubernetes { 
      yaml libraryResource('KubernetesPod.yaml')
    }
  }
  stages {
    stage('Run docker') {
      steps {
        container('docker') {
          sh 'echo hello from docker'
        }
      }
    }
  }
}
